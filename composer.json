{
  "name": "keboola/syrup-php-cli",
  "description": "Command line client for Keboola jobs",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "marc raiser",
      "email": "marc@keboola.com"
    }
  ],
  "require": {
    "symfony/console": "^3.2",
    "keboola/syrup-php-client": "^4.1",
    "symfony/process": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": { "Keboola\\Syrup\\": "src/Keboola/Syrup" }
  },
  "autoload-dev": {
    "psr-4": { "Keboola\\Syrup\\Test\\": "tests/Keboola/Syrup" }
  },
  "config": {
    "platform": {
      "php": "7.0.3"
    }
  }
}
